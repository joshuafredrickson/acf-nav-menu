{
  "name": "joshuafredrickson/acf-nav-menu",
  "type": "wordpress-plugin",
  "description": "A nav menu field for ACF.",
  "license": "MIT",
  "keywords": [
    "wordpress",
    "acf",
    "acf-field",
    "wp-nav-menu"
  ],
  "autoload": {
    "psr-4": {
      "Jf\\AcfNavMenu\\": "src/"
    }
  },
  "require": {
    "php": "^7.0|^8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4"
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor,examples --extensions=php --standard=PSR12 ."
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
